<?xml version="1.0"?>
<extension key="fixmembershipfileids" type="module">
  <file>fixmembershipfileids</file>
  <name>fixmembershipfileids</name>
  <description>Fixes a feature in CiviCRM where the link between a file and a membership is wrong.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alain Benbassat</author>
    <email>alain@businessandcode.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/AlainBenbassat/fixmembershipfileids</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-10-19</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.51</ver>
  </compatibility>
  <comments>See api Membership.Fixfileids</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Fixmembershipfileids</namespace>
    <format>22.10.0</format>
    <angularModule>crmFixmembershipfileids</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
